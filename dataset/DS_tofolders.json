{
	"name": "DS_tofolders",
	"properties": {
		"linkedServiceName": {
			"referenceName": "LS_ADLS",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"foldername": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "Binary",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"folderPath": {
					"value": "@concat(dataset().foldername)",
					"type": "Expression"
				},
				"fileSystem": "kdcontainer"
			}
		}
	}
}